{
  "name": "@cardinal/onchain-storage",
  "version": "1.2.0",
  "description": "Cardinal onchain storage SDK",
  "keywords": [
    "solana",
    "anchor",
    "cardinal",
    "onchain",
    "storage"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": "git@github.com:cardinal-labs/cardinal-onchain-storage.git",
  "author": "Giannis Chatziveroglou <giannis@cardinal.so>",
  "bugs": {
    "url": "https://github.com/cardinal-labs/cardinal-onchain-storage/issues",
    "email": "info@cardinal.so"
  },
  "homepage": "https://cardinal.so",
  "license": "AGPL-3.0",
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,json,jsx,html,css,md}": "prettier --write"
  },
  "resolutions": {
    "bn.js": "^5.2.0",
    "@types/bn.js": "^5.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "rm -fr dist/ && tsc -P tsconfig.cjs.json && tsc -P tsconfig.esm.json",
    "clean": "rm -fr dist/",
    "idl:generate": "./generate-idls.sh",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ src/index.ts",
    "test": "jest --detectOpenHandles",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --fix"
  },
  "dependencies": {
    "@cardinal/common": "^5.0.2",
    "@project-serum/anchor": "^0.25.0",
    "@solana/web3.js": "^1.66.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4",
    "@types/jest": "^29.2.0",
    "@types/node": "^17.0.10",
    "@types/prettier": "^2.4.3",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "bn.js": "^5.2.0",
    "eslint": "^8.26.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.0",
    "jest": "^29.2.1",
    "jest-junit": "^15.0.0",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.5.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "packageManager": "yarn@3.1.1"
}
